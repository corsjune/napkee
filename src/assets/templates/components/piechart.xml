<?xml version="1.0" encoding="utf-8"?>
<component>
	<html>
	<![CDATA[
<div id="${id}" class="napkeeComponent napkeePiechart"></div>
	]]>
	</html>
	<css>
	<![CDATA[
#${id}{
	left:${left};
	top:${top};
	width:${width};
	height:${height};
	z-index:${z};
}
	]]>
	</css>
	<jsdocready>
	<![CDATA[
var chartOpt${id} = [10,20];
$("#${id}").sparkline(chartOpt${id}, {type: "pie", width: "${width}px", height: "${height}px", sliceColors:["#FF0000","#0000FF"]});
	]]>
	</jsdocready>
	<jsimport>
		<jsfile>jquery</jsfile>
		<jsfile>jquery-ui</jsfile>
	</jsimport>
	<cssimport>
		<cssfile>jquery-ui</cssfile>
	</cssimport>
</component>