<?xml version="1.0" encoding="utf-8"?>
<component>
	<html>
	<![CDATA[
<div id="${id}" class="napkeeComponent napkeeTooltip" title="${label}"></div>
	]]>
	</html>
	<css>
	<![CDATA[
#${id}{
	left:${left};
	top:${calcTop};
	width:${width};
	height:${height};
	z-index:${z};
}
	]]>
	</css>
	<jsdocready>
	<![CDATA[
	$("#${id}").tooltip({
		trigger: "manual",
		placement: "${placement}"
	}).tooltip('show');
	]]>
	</jsdocready>
	<jsimport>
	</jsimport>
	<cssimport>
	</cssimport>
	
</component>